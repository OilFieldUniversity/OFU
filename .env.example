# Copy this file to .env and fill in your credentials

# Flask secret key
FLASK_SECRET=replace_with_a_random_secret

# Google OAuth credentials
# Create credentials at: https://console.developers.google.com/apis/credentials
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Facebook OAuth credentials
# Create credentials at: https://developers.facebook.com/apps
FACEBOOK_CLIENT_ID=your_facebook_app_id
FACEBOOK_CLIENT_SECRET=your_facebook_app_secret

# Optional: change host/port if needed
#FLASK_RUN_HOST=127.0.0.1
#FLASK_RUN_PORT=5000